{
  "name": "imgs-server",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "framer-motion": "^2.8.0"
  },
  "scripts": {
